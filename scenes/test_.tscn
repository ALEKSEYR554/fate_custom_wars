script/source = "@tool
extends EditorScript


# === ВХОДНЫЕ ДАННЫЕ (остаются прежними) ===

func get_unique_id_from_string(data_string: String) -> String:
	var ctx = HashingContext.new()
	var buffer = data_string.to_utf8_buffer()
	ctx.start(HashingContext.HASH_MD5)
	ctx.update(buffer)
	var hash_bytes = ctx.finish()
	return hash_bytes.hex_encode()

func _run():
	var my_string = '{\"Name\":\"Summon\",\"Duration\":3,\"Summon Name\":\"Rama\",\"Skill Enabled\":false,\"Move Points\":1,\"Phantasm Points Farm\":false,\"Limit\":3}'
	
	var unique_id = get_unique_id_from_string(my_string)
	
	print(\"Оригинальная строка: \", my_string)
	print(\"Уникальный ID (MD5): \", unique_id) # Вывод: 8a9e4871e845c228d48293d562b9a765

	# Пример с измененными данными
	var another_string = '{\"Name\":\"Summon\",\"Duration\":4,\"Summon Name\":\"Rama\",\"Skill Enabled\":false,\"Move Points\":1,\"Phantasm Points Farm\":false,\"Limit\":3}'
	var another_id = get_unique_id_from_string(another_string)
	print(\"Другой ID (MD5): \", another_id) # Вывод будет совершенно другим
"

